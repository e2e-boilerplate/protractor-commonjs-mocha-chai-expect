{
  "name": "protractor-commonjs-mocha-chai-ecpect",
  "version": "1.0.0",
  "description": "Protractor end-to-end test automation boilerplate, using Commonjs, Mocha, Chai and Expect.",
  "scripts": {
    "lint": "npx eslint --ext .js ./ --fix",
    "upadte-webderiver": "npx webdriver-manager update",
    "upadte-webderiver-ci": "npx webdriver-manager update --versions.chrome=79.0.3945.36",
    "test": "npx protractor"
  },
  "keywords": [
    "protractor",
    "commonjs",
    "mocha",
    "chai",
    "boilerplate",
    "example",
    "e2e",
    "e2e test"
  ],
  "author": "Girma Nigusse <xgirma@gmail.com> (https://github.com/xgirma)",
  "license": "MIT",
  "devDependencies": {
    "chai": "4.2.0",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-import": "2.20.1",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.3",
    "mocha": "7.0.1",
    "prettier": "1.19.1",
    "pretty-quick": "2.0.1",
    "protractor": "5.4.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
